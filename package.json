        },
        "./batch-request": {
            "types": "./dist/utils/core/BatchRequest.d.ts",
            "import": "./dist/utils/core/BatchRequest.js"
        },
        "./http-status": {
            "types": "./dist/utils/helpers/HttpStatusHelper.d.ts",
            "import": "./dist/utils/helpers/HttpStatusHelper.js"
        },
        "./url-builder": {
            "types": "./dist/utils/helpers/UrlBuilder.d.ts",
            "import": "./dist/utils/helpers/UrlBuilder.js"
        }
    },
    "scripts": {
        "build": "tsc -p tsconfig.json",
        "clean": "node -e \"require('fs').rmSync('dist', { recursive: true, force: true });\"",
        "lint": "tsc --noEmit",
        "test": "npm run lint && npm run build && node --test tests/**/*.test.js",
        "prepublishOnly": "npm run lint && npm run clean && npm run build"
    },
    "files": [
        "dist",
        "bin"
    ],
    "keywords": [
        "typescript",
        "http-client",
        "logger",
        "utilities",
        "formerly @sebamar88/utils"
    ],
    "repository": {
        "type": "git",
        "url": "git+https://github.com/sebamar88/utils.git"
    },
    "bugs": {
        "url": "https://github.com/sebamar88/utils/issues"
    },
    "homepage": "https://github.com/sebamar88/utils#readme",
    "author": "SebastiÃ¡n Martinez",
    "license": "MIT",
    "sideEffects": false,
    "publishConfig": {
        "access": "public"
    },
    "engines": {
        "node": ">=18"
    },
    "dependencies": {
        "cross-fetch": "^4.1.0"
    },
    "devDependencies": {
        "@types/node": "^25.0.3",
        "typescript": "^5.9.3"
    },
    "peerDependencies": {
        "typescript": ">=5.9.0"
    },
    "peerDependenciesMeta": {
        "typescript": {
            "optional": true
        }
    }
